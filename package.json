{
  "name": "schwab-node",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Perry Angelora <perry.angelora@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MisterPea/schwab-node"
  },
  "description": "A Node.js wrapper for Schwab's Market Data API â€” get quotes, option chains, price history, and more with minimal setup",
  "keywords": [
    "schwab",
    "schwab-api",
    "charles-schwab",
    "trading-api",
    "brokerage-api",
    "finance-api",
    "oauth2",
    "authentication",
    "nodejs",
    "typescript"
  ],
  "engines": {
    "node": ">=20.6.0"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "schwab-node-certs": "./dist/scripts/setup-certs.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsx watch src/index.ts",
    "test": "vitest run --exclude \"test/**/*.live.test.ts\"",
    "test:contract": "vitest run -c vitest.contract.config.ts",
    "test:live:smoke": "vitest run -c vitest.live-smoke.config.ts",
    "prepack": "npm run build",
    "certs:install": "node --import tsx src/scripts/setup-certs.ts",
    "lint": "npx eslint .",
    "coverage": "vitest run --coverage",
    "build": "tsc"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.0",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.39.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.15"
  },
  "dependencies": {
    "express": "^5.2.1"
  }
}